{
  "name": "devworkout/scaffold-docker",
  "description": "",
  "keywords": [
    "devworkout",
    "scaffold"
  ],
  "homepage": "https://github.com/devworkout/scaffold-docker",
  "license": "MIT",
  "authors": [
    {
      "name": "Vladislav Novik",
      "email": "librevlad@gmail.com",
      "homepage": "https://serpentine.io",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "nunomaduro/laravel-console-task": "^1.2",
    "afterflow/framework": "dev-master"
  },
  "require-dev": {
    "laravel/framework": "5.*",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "DevWorkout\\ScaffoldDocker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DevWorkout\\ScaffoldDocker\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "DevWorkout\\ScaffoldDocker\\ScaffoldServiceProvider"
      ]
    }
  }
}
